<?xml version="1.0"?>
<ruleset name="Go6d PMD Ruleset" xmlns="http://pmd.sf.net/ruleset/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://pmd.sf.net/ruleset/1.0.0 http://pmd.sf.net/ruleset_xml_schema.xsd"
  xsi:noNamespaceSchemaLocation="http://pmd.sf.net/ruleset_xml_schema.xsd">
  <description>Go6d PMD Ruleset</description>

  <rule ref="code_analyze_and_test/config/pmd/rulesets/basic.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/braces.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/clone.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/comments.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/controversial.xml">
    <!-- sometimes conflict with rule of UnnecessaryConstructor -->
    <exclude name="AtLeastOneConstructor" />
    <exclude name="DataflowAnomalyAnalysis" />
  </rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/coupling.xml">
  <!-- to suppress warnings-->
    <exclude name="LoosePackageCoupling" />
    <exclude name="LawOfDemeter" />
  </rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/design.xml">
  </rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/empty.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/finalizers.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/imports.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/j2ee.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/javabeans.xml">
  </rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/junit.xml"></rule>
  <rule
    ref="code_analyze_and_test/config/pmd/rulesets/logging-jakarta-commons.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/logging-java.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/migrating.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/naming.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/optimizations.xml">
  <!-- too many final -->
    <exclude name="MethodArgumentCouldBeFinal"></exclude>
  </rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/strictexception.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/strings.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/sunsecure.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/typeresolution.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/unnecessary.xml"></rule>
  <rule ref="code_analyze_and_test/config/pmd/rulesets/unusedcode.xml"></rule>
</ruleset>
